<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ImageToText" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_grotto</item>
        <item name="colorPrimaryVariant">@color/blue_green</item>
        <item name="colorPrimaryDark">@color/navy_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_grotto</item>
        <item name="colorSecondaryVariant">@color/blue_green</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->

        <item name="android:statusBarColor" tools:targetApi="l">@android:color/darker_gray</item>
        <!-- Customize your theme here. -->
        <item name="android:letterSpacing">0</item>
        <item name="textAllCaps">false</item>

    </style>
</resources>